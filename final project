      "source": [
        "# Summarize the process and results\n",
        "\n",
        "print(\"Task Completed: Multilingual Dataset Fine-tuning\")\n",
        "print(\"-----------------------------------------------\")\n",
        "print(\"We have successfully:\")\n",
        "print(\"1. Loaded and preprocessed the dataset.\")\n",
        "print(\"2. Translated the dataset into target languages (Spanish and French in this example).\")\n",
        "print(\"3. Prepared the dataset for fine-tuning.\")\n",
        "print(f\"4. Loaded the pre-trained model: {model_name}\")\n",
        "print(\"5. Set up the fine-tuning process.\")\n",
        "# Add evaluation results if available\n",
        "if 'results' in locals():\n",
        "    print(\"\\nEvaluation Results:\")\n",
        "    for key, value in results.items():\n",
        "        print(f\"- {key}: {value:.4f}\")\n",
        "else:\n",
        "    print(\"\\nEvaluation was not performed or completed.\")\n",
        "\n",
        "print(\"\\n-----------------------------------------------\")\n",
        "print(\"How to use the fine-tuned model:\")\n",
        "print(\"-----------------------------------------------\")\n",
        "print(\"The fine-tuned model is now stored in the 'model' variable.\")\n",
        "print(f\"You can save the model and tokenizer to a local directory using:\")\n",
        "print(\"model.save_pretrained('./fine_tuned_model')\")\n",
        "print(\"tokenizer.save_pretrained('./fine_tuned_model')\")\n",
        "\n",
        "print(\"\\nTo load the fine-tuned model later:\")\n",
        "print(\"from transformers import AutoTokenizer, AutoModelForConditionalGeneration\")\n",
        "print(\"tokenizer = AutoTokenizer.from_pretrained('./fine_tuned_model')\")\n",
        "print(\"model = AutoModelForConditionalGeneration.from_pretrained('./fine_tuned_model')\")\n",
        "\n",
        "print(\"\\nExample of using the fine-tuned model for translation/generation:\")\n",
        "\n",
        "def generate_response(prompt_text, model, tokenizer, max_length=128):\n",
        "    \"\"\"Generates a response using the fine-tuned model.\"\"\"\n",
        "    input_encoding = tokenizer(\n",
        "        prompt_text,\n",
        "        padding='max_length',\n",
        "        truncation=True,\n",
        "        max_length=max_length,\n",
        "        return_tensors='pt'\n",
        "    ).to(model.device) # Move to the same device as the model\n",
        "\n",
        "    generated_tokens = model.generate(\n",
        "        input_ids=input_encoding['input_ids'],\n",
        "        attention_mask=input_encoding['attention_mask'],\n",
        "        max_length=max_length,\n",
        "        num_beams=4, # Example of using beam search\n",
        "        early_stopping=True\n",
        "    )\n",
        "\n",
        "    generated_text = tokenizer.decode(generated_tokens[0], skip_special_tokens=True)\n",
        "    return generated_text\n",
        "\n",
        "# Example usage (replace with an actual prompt you want to test)\n",
        "test_prompt_en = \"prompt: How do I get certified for a course? response:\"\n",
        "# You might need to adapt the prompt format based on how you structured your training data\n",
        "# For instruction tuning, the prompt should match the training input format\n",
        "\n",
        "print(f\"\\nTest Prompt (English): {test_prompt_en}\")\n",
        "# Ensure the model is on CPU or GPU before generating\n",
        "# model.to('cuda') # Uncomment if you have a GPU\n",
        "\n",
        "generated_response_en = generate_response(test_prompt_en, model, tokenizer)\n",
        "print(f\"Generated Response (English): {generated_response_en}\")\n",
        "\n",
        "# Example for a translated prompt (assuming 'es' for Spanish)\n",
        "# test_prompt_es = \"prompt: ¿Cómo me certifico para un curso? response:\"\n",
        "# generated_response_es = generate_response(test_prompt_es, model, tokenizer)\n",
        "# print(f\"Generated Response (Spanish): {generated_response_es}\")\n",
        "\n",
        "print(\"\\nNote: The example generation is commented out. Uncomment and adapt the prompt format to test the model.\")"
      ],
