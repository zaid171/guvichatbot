        "# 1. Check for and handle any missing values\n",
        "print(\"Checking for missing values:\")\n",
        "display(df.isnull().sum())\n",
        "\n",
        "# Since there are no missing values, no handling is needed.\n",
        "\n",
        "# 2. Inspect data types\n",
        "print(\"\\nChecking data types:\")\n",
        "display(df.dtypes)\n",
        "\n",
        "# The data types are already 'object', which is suitable for strings. No conversion needed.\n",
        "\n",
        "# 3. Basic text cleaning\n",
        "print(\"\\nPerforming basic text cleaning (removing leading/trailing whitespace):\")\n",
        "df['prompt'] = df['prompt'].str.strip()\n",
        "df['response'] = df['response'].str.strip()\n",
        "\n",
        "# Display the first few rows after cleaning to verify\n",
        "print(\"\\nFirst 5 rows after cleaning:\")\n",
        "display(df.head())"
